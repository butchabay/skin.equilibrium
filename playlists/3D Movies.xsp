<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>3D Movies</name>
    <match>one</match>
    <rule field="filename" operator="contains">
        <value>.3DBD.</value>
    </rule>
    <rule field="genre" operator="is">
        <value>3D</value>
    </rule>
</smartplaylist>
