<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Last Played Movie</name>
    <match>all</match>
    <rule field="inprogress" operator="false" />
    <rule field="lastplayed" operator="inthelast">
        <value>10 days</value>
    </rule>
    <group>none</group>
    <limit>10</limit>
</smartplaylist>
